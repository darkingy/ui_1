# 3D 그래프 생성
fig = plt.figure(figsize=(10, 8))
ax = fig.add_subplot(111, projection='3d')

# 데이터 세트 1
ax.scatter(x, y, z, c= 'b', label='Data Set 1', marker='o')

# 데이터 세트 2
ax.scatter(x1, y1, z1, c='g', label='Data Set 2', marker='s')

# 데이터 세트 3
ax.scatter(x2, y2, z2, c='r', label='Data Set 3', marker='^')

# 데이터 세트 4
ax.scatter(x3, y3, z3, c='y', label='Data Set 4', marker='D')

# 데이터 세트 5
ax.scatter(x4, y4, z4, c='k', label='Data Set 5', marker='P')

# 데이터 점을 이어서 표시
ax.view_init(20, -30)
ax.plot(x, y, z, linestyle='-', color='blue')
ax.plot(x1, y1, z1, linestyle='-', color='green')
ax.plot(x2, y2, z2, linestyle='-', color='red')
ax.plot(x3, y3, z3, linestyle='-', color='purple')
ax.plot(x4, y4, z4, linestyle='-', color='orange')

# 그래프 제목과 레이블 설정
ax.set_title('3D 그래프')
ax.set_xlabel('X 축')
ax.set_ylabel('Y 축')
ax.set_zlabel('Z 축')
ax.legend()

# 그래프 표시
plt.show()

